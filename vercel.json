{
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "installCommand": "npm i",
  "outputDirectory": "dist",
  "public": true,
  "git": {
    "deploymentEnabled": {
      "master": false
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "crons": [{
    "path": "tasks/jobs/purchaseReminder",
    "schedule": "0 12 * * *"
  }]
}